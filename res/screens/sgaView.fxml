<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.net.URL?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.text.Text?>

<GridPane alignment="CENTER" hgap="10.0" prefHeight="365.0" styleClass="root" vgap="10.0"
      xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
      fx:controller="src.code.controller.SgaFormController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <padding>
    <Insets bottom="10.0" left="25.0" right="25.0" top="25.0" />
  </padding>
  <HBox alignment="BOTTOM_RIGHT" spacing="10.0" GridPane.columnIndex="0" GridPane.rowIndex="6" />
  <Label maxWidth="450.0" text="Select a year and, optionally, a previous year." GridPane.columnIndex="0" GridPane.rowIndex="0" />
  <HBox prefHeight="24.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
    <children>
      <Label text="Year" />
      <ChoiceBox fx:id="yearsList">
        <items>
          <FXCollections fx:factory="observableArrayList">
            <String fx:value="Item 1" />
            <String fx:value="Item 2" />
          </FXCollections>
        </items>
      </ChoiceBox>
    </children>
  </HBox>
  <HBox prefHeight="21.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="2">
    <children>
      <CheckBox fx:id="wantPrev" mnemonicParsing="false" text="Select if you want to use previous year." />
    </children>
  </HBox>
  <rowConstraints>
    <RowConstraints maxHeight="16.0" minHeight="0.0" prefHeight="0.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="40.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="21.0" minHeight="10.0" prefHeight="21.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="21.0" minHeight="10.0" prefHeight="21.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="-1.0" minHeight="10.0" prefHeight="21.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="31.0" minHeight="10.0" prefHeight="31.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="10.0" minHeight="0.0" prefHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <stylesheets>
    <URL value="@../Login.css" />
  </stylesheets>
  <TableView fx:id="sgaTable" prefHeight="149.0" prefWidth="511.0" GridPane.columnIndex="0" GridPane.rowIndex="4">
    <columns>
      <TableColumn fx:id="colName" prefWidth="75.0" text="Name" />
      <TableColumn fx:id="colCat" prefWidth="75.0" text="Category" />
      <TableColumn fx:id="colPyReq" maxWidth="5000.0" minWidth="10.0" prefWidth="94.0" text="PY Requested" />
      <TableColumn fx:id="colPyApp" maxWidth="5000.0" minWidth="10.0" prefWidth="85.0" text="PY Approved" />
      <TableColumn fx:id="colCyReq" maxWidth="5000.0" minWidth="10.0" prefWidth="90.0" text="CY Requested" />
      <TableColumn fx:id="colCyApp" maxWidth="5000.0" minWidth="10.0" prefWidth="90.0" text="CY Approved" />
    </columns>
  </TableView>
  <Button mnemonicParsing="false" onAction="#goBack" text="Back" GridPane.columnIndex="0" GridPane.rowIndex="5" />
  <Button mnemonicParsing="false" onAction="#submit" text="Submit" GridPane.columnIndex="0" GridPane.rowIndex="3" />
</GridPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.net.URL?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.text.Text?>

<GridPane alignment="CENTER" hgap="10.0" styleClass="root" vgap="10.0"
      xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
      fx:controller="src.code.controller.BudgetItemController">
  <Text fx:id="actiontarget" GridPane.columnIndex="0" GridPane.rowIndex="1" />
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <padding>
    <Insets bottom="10.0" left="25.0" right="25.0" top="25.0" />
  </padding>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <stylesheets>
    <URL value="@../Login.css" />
  </stylesheets>
  <HBox alignment="BOTTOM_RIGHT" spacing="10.0" GridPane.columnIndex="0" GridPane.rowIndex="2">
    <children>
      <Button onAction="#goBack" mnemonicParsing="false" text="Back" />
      <Button mnemonicParsing="false" text="OK" />
    </children>
  </HBox>
  <Label text="Add Budget Item" GridPane.columnIndex="0" GridPane.rowIndex="0" />
  <GridPane prefWidth="500.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
    <children>
      <Text text="Excel Spreadsheet" GridPane.columnIndex="0" GridPane.rowIndex="0" />
      <TextField prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
      <Text text="Email" GridPane.columnIndex="0" GridPane.rowIndex="1" />
      <TextField prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Text text="GPA" GridPane.columnIndex="0" GridPane.rowIndex="2" />
      <TextField prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
    </children>
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="120.0" minWidth="10.0" prefWidth="120.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="98.0" minWidth="10.0" prefWidth="44.0" />
    </columnConstraints>
    <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
  </GridPane>
</GridPane>
